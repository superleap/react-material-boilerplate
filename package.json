{
  "name": "react-material-boilerplate",
  "version": "0.2.1",
  "description": "React material boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack-development.config.js --progress --inline --colors --open",
    "build": "webpack --config webpack-production.config.js --progress --colors",
    "lint": "eslint src && echo \"eslint: no lint errors\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:superleap/react-material-boilerplate.git"
  },
  "author": "TED Vortex",
  "license": "UNLICENSED",
  "homepage": "https://github.com/superleap/react-material-boilerplate",
  "dependencies": {
    "babel-standalone": "^6.7.4",
    "eslint": "^2.10.0",
    "html-webpack-plugin": "^2.17.0",
    "material-ui": "^0.15.0",
    "material-ui-community-icons": "^0.15.0",
    "npm-install-webpack-plugin": "^3.1.3",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-helmet": "^3.1.0",
    "react-tap-event-plugin": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.15.0",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "npm-install-webpack-plugin": "^3.1.1",
    "react-addons-css-transition-group": "^15.0.2",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-react-jsx"
    ],
    "ignore": []
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    }
  }
}
